# RabbitMQ Configuration
# Copy this file to .env.rabbitmq and fill in your actual values
RABBITMQ_DEFAULT_USER=your_rabbitmq_user
RABBITMQ_DEFAULT_PASS=your_rabbitmq_password
RABBITMQ_DEFAULT_VHOST=/

# Management Plugin
RABBITMQ_MANAGEMENT_ENABLED=true

# Security
RABBITMQ_AUTH_BACKENDS=rabbit_auth_backend_internal

# Performance
RABBITMQ_DISK_FREE_LIMIT=2000000